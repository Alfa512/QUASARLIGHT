<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="HandheldFriendly" content="true">
    <meta name="author" content="">
    @RenderSection("Title", false)

    <link href="~/Content/img/favicon.png" rel="shortcut icon" type="image/x-icon" />

    @Scripts.Render("~/bundles/layout.top")
    @Styles.Render("~/Content/css")
</head>
<body ng-app="dialogue-app">
    @Scripts.Render("~/bundles/layout")

    <main class="cd-main-content ">

        @Html.Partial("_TopBar")

        @RenderBody()
        @Html.Partial("_Footer")
        @Scripts.Render("~/bundles/layout.bottom")
        @RenderSection("Scripts", false)
    </main>
    <noscript>
        <div>
            @*<img src="https://mc.yandex.ru/watch/33702239" style="position: absolute; left: -9999px;" alt="" />*@
        </div>
    </noscript>
</body>
</html>